<script type="text/javascript">
setTimeout(function() {
var movieDomain = 'http://' + location.href.split('/')[2] + '/';
var flvfile = params.mv;
var cc = location.href.split('/')[3];
if (cc == 'jp' && location.href.indexOf('wii.com') != -1) {
	flvfile = 'rtmp://fms.wii.com/wii/_definst_/' + params.mv + '.flv';
} else {
	flvfile = '../../movies/flv/animalcrossing/' + params.mv + '.flv';
}
var so = new SWFObject("/swf/flvplayer_iwata.swf", "flvplayer", params.width, params.height, "7", "#666666");
if (typeof params.mv != 'undefined') {
so.addVariable("flv", flvfile);
so.addVariable("totalTime", params.tt);
var _title = document.title;
so.write("swfcontent");
document.title = _title;
}
}, 800);

</script>

