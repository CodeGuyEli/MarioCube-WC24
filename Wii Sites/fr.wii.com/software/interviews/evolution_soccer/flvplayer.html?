<script type="text/javascript">
setTimeout(function() {
var flvfile='http://wiimedia.nintendo-europe.com/wii.com/sw_interviews/'+ mv +'.flv'
var so = new SWFObject("swf/smg.swf", "flvplayer", params.width, params.height, "7", "#666666");
so.addVariable("player", "swf/flvplayer_inside.swf");
so.addVariable("lang", mv);
so.addVariable("xml", "../SMG/xml/" + mv + ".xml");

if (typeof params.mv != 'undefined') {
so.addVariable("flv", flvfile);
so.addVariable("totalTime", params.tt);
var _title = document.title;
so.write("swfcontent");
document.title = _title;
}
}, 800);

</script>

